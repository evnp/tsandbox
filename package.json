{
  "name": "tsandbox",
  "version": "0.0.1",
  "description": "A TypeScript code sandbox with Jest, ESLint, Prettier.",
  "scripts": {
    "build": "tsc",
    "test": "jest --watch",
    "lint": "eslint src/ test/",
    "format": "eslint --fix src/ test/",
    "edit": "run() { vim -O \"./test/$1.test.ts\" \"./src/$1.ts\" -c \"bel term npm t $1\" -c \"winc l\"; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evnp/tsandbox.git"
  },
  "author": "Evan Purcer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
